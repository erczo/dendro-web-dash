{
  "name": "dendro-web-dash",
  "version": "0.0.5",
  "description": "Project Dendro web dashboards.",
  "license": "BSD-2-Clause-FreeBSD",
  "author": "J. Scott Smith <scott@newleafsolutionsinc.com>",
  "main": "dist/server/main.js",
  "scripts": {
    "babel-server": "babel --presets \"es2015-node6\" --no-babelrc src/server -d dist/server",
    "build": "npm run build-client:prod && npm run build-server",
    "build-client": "npm run clean-client && npm run webpack",
    "build-client:dev": "NODE_ENV=development npm run build-client",
    "build-client:prod": "NODE_ENV=production npm run build-client",
    "build-server": "npm run lint-server && npm run clean-server && npm run babel-server && npm run copy-views",
    "clean": "npm run clean-client && npm run clean-server",
    "clean-client": "rm -rf dist/public/client/*",
    "clean-server": "rm -rf dist/server/*",
    "copy-views": "cp -R src/server/views dist/server",
    "lint": "eslint src",
    "lint-client": "eslint src/client",
    "lint-server": "eslint src/server",
    "start": "node ./dist/server/main.js",
    "start:dev": "NODE_ENV=development npm start",
    "start:prod": "NODE_ENV=production npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack --config ./config/webpack.config.js"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "config": "^1.24.0",
    "express": "^4.14.0",
    "font-awesome": "^4.6.3",
    "moment": "^2.18.1",
    "pug": "^2.0.0-beta6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "chroma-js": "^1.2.2",
    "css-loader": "^0.26.4",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "feathers": "^2.0.3",
    "feathers-hooks": "^1.7.1",
    "feathers-socketio": "^1.4.2",
    "file-loader": "^0.9.0",
    "highcharts": "^5.0.7",
    "highcharts-more": "^0.1.2",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.4",
    "load-google-maps-api": "0.0.2",
    "localforage": "^1.4.3",
    "lodash": "^4.17.4",
    "lodash.debounce": "^4.0.8",
    "mathjs": "^3.8.1",
    "socket.io-client": "^1.7.2",
    "style-loader": "^0.13.2",
    "tether": "^1.3.7",
    "url-loader": "^0.5.7",
    "vue": "^2.2.5",
    "vue-hot-reload-api": "^2.0.6",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^10.3.0",
    "vue-router": "^2.1.1",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.2.5",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.8.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "browserDependencies": {
    "dendro-bootstrap": "erczo/dendro-bootstrap",
    "weather-icons": "erikflowers/weather-icons"
  },
  "optionalDependencies": {
    "fsevents": "^1.0.0"
  }
}
